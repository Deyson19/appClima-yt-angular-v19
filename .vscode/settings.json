{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "hide-files.files": [],
  "angular-schematics.schematicsDefaultOptions": {
    "angular-*": {
      "skipStyle": true
    },
    "angular-component": {
      "skipDisplayBlock": true
    }
  },
  "exportall.config.folderListener": ["/src/app/shared"]
}
